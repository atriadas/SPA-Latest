/* DELETE AND LEFT ADD */

#leftaddstyle{
    padding: 1.5% 5% ;
    background:linear-gradient(to bottom,white,rgb(224, 224, 225));
    display: inline-block;
    text-align: center;
    border-radius: 5px;
    border: 1px solid rgb(190, 190, 190);
    font-size: 10pt;
    font-weight: 400;
}

#deletestyle{
    padding:1.5% ;
    background:linear-gradient(to bottom,white,rgb(224, 224, 225));
    display: inline-block;
    text-align: center;
    border-radius: 5px;
    border: 1px solid rgb(190, 190, 190);
    font-size: 80%;
    font-weight: 400;
}

#cancelstyle{
    padding:1% ;
    background:linear-gradient(to bottom,white,rgb(224, 224, 225));
    display: inline-block;
    text-align: center;
    border-radius: 5px;
    border: 1px solid rgb(190, 190, 190);
    font-size: 70%;
    font-weight: 400;
}

#savestyle{
    font-size: 75%;
     width: 40%;
      padding:4%;
}

#savestyle:disabled{
    font-size: 75%;
     width: 40%;
      padding:4%;
    background-color: grey;
    border-color: darkgrey;
}


/* RIGHT ADD */

#rightaddstyle{
    padding:5px;
    background:linear-gradient(to bottom,white,rgb(224, 224, 225));
    display: inline-block;
    text-align: center;
    border-radius: 5px;
    border: 1px solid rgb(190, 190, 190);
    font-size: 100%;
    font-weight: 400;
    width: 9%;
}
#rightaddstyle1:disabled {
    padding:4.3px 0px;
    background: linear-gradient(to bottom, white, rgb(224, 224, 225));
    display: inline-block;
    text-align: center;
    border-radius: 5px;
    border: 1px solid rgb(190, 190, 190);
    font-weight: 400;
    width: 80%;
    font-size: 72%;
    color:black;
}

#rightaddstyle1:enabled {
    padding:4.3px 0px;
    background: blue;
    display: inline-block;
    text-align: center;
    border-radius: 5px;
    border: 1px solid rgb(190, 190, 190);
    font-weight: 400;
    width: 80%;
    font-size: 72%;
    color: white;

.ng-select{
    width:90%;
    float: left;
}

.pageAnchor{
    cursor: pointer;
}