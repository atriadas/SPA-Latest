.project-tab #tabs .nav-tabs .nav-item.show .nav-link, 
.nav-tabs .nav-link.active {
    color:black !important;
    background-color: transparent;
    border-color: transparent transparent #f3f3f3;
    border-bottom: 3px solid rgb(42, 119, 184)!important;
    font-size: 9pt;
    text-align: left;
    padding: 8px 0px 0px 0px; 
}

.project-tab #tabs .nav-tabs .nav-item.show .nav-link, 
.nav-tabs .nav-link.active .badge{
    padding: 5px 9px;
    border-radius: 20%;
    background-color:rgb(42, 119, 184)!important;
    color: white!important;
}

a#nav-profile-tab{
    color:lightgrey;
    padding: 8px 2px 8px 2px;
    font-size: 9pt;
}

a#nav-home-tab{
    color: lightgrey;
    padding: 8px 2px 8px 2px;
    font-size: 9pt;
}

#newGroup {   
    text-align: center;
    padding-top: 40%;
}

#grpbtn{
    height:0.4in;
    border: 1px solid lightgrey;
    font: rgb(22, 22, 22);
}

.nav-fill .nav-item{
    text-align: left;
    display: inline-block !important;
}

#noNames{
    position: relative;
    text-align: center;
    height: 3.68in;
}

#noNames #smaltex{
    top: 50%;
    left: 50%;
    position: absolute;
    margin: 0;
    transform: translate(-50%,-50%);
}

#noNames #bigtex{
    top: 42%;
    left: 50%;
    position: absolute;
    margin: 0;
    transform: translate(-50%,-50%);
}

.badge {         
    padding: 5px 9px;
    border-radius: 20%;
    background-color: lightgray;
    color:black;  
  }
 
#nav-profile-tab{
    flex-basis: unset;
    flex-grow: unset;
    flex-shrink: unset;
}

#nav-home-tab{
    flex-basis: unset;
    flex-grow: unset;
    flex-shrink: unset;
}

table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
    width: 100%;
  }
  
  td, 
  th {   
    text-align: left;
    padding: 5px;
  }
  
  tr:nth-child(even) {
    background-color: #eeeded;
  }
  
  #exampleModal{
      margin-top: 3.3in;
      border-radius: 30%;
  }

#alertBox{
    color: rgb(66, 201, 111);
    background-color: rgb(208, 252, 208);
    border: 1px solid rgb(66, 201, 111);
}

#failalertBox{
    color:red;
    background-color: rgb(252, 207, 207);
    border: 1px solid red;   
}

.MainModal{
    margin-top: 28px;
}

#leftList{
    width: 100%;
    border: none;
    text-align: left;
    padding: 8px;
    background-color: white;
    /* word-wrap:break-word; */
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;  
}

#leftList:focus{
    color: rgb(0, 89, 255);
    background-color: rgb(229, 242, 245);
    outline: none;
        /* word-wrap:break-word; */
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

#groupNameInput:invalid{
 color: red;
}

